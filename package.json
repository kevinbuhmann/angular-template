{
  "name": "angular-template",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "nodemon dist/server/server.js",
    "test": "ng test --watch=false --code-coverage",
    "lint": "ng lint --type-check",
    "watch": "parallelshell \"npm run client:watch\" \"npm run server:watch\"",
    "build": "npm run lint && npm run client:build && npm run server:build",
    "build--prod": "npm run lint && npm run client:build--prod && npm run server:build--prod",
    "build--full": "npm run build--prod && npm test",
    "client:watch": "ng build --watch",
    "client:build": "ng build",
    "client:build--prod": "ng build --prod",
    "server:watch": "webpack --config ./build/webpack.server.dev.ts --progress --watch",
    "server:build": "webpack --config ./build/webpack.server.dev.ts --progress",
    "server:build--prod": "webpack --config ./build/webpack.server.prod.ts --progress --bail"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.2.4",
    "@angular/common": "4.2.4",
    "@angular/compiler": "4.2.4",
    "@angular/core": "4.2.4",
    "@angular/forms": "4.2.4",
    "@angular/http": "4.2.4",
    "@angular/platform-browser": "4.2.4",
    "@angular/platform-browser-dynamic": "4.2.4",
    "@angular/platform-server": "4.2.4",
    "@angular/router": "4.2.4",
    "compression": "1.6.2",
    "core-js": "2.4.1",
    "express": "4.15.3",
    "express-logging": "1.1.1",
    "logops": "2.0.0",
    "rx-routes": "0.2.0",
    "rxjs": "5.4.1",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "1.1.3",
    "@angular/compiler-cli": "4.2.4",
    "@angular/language-service": "4.2.4",
    "@types/express": "4.0.36",
    "@types/html-minifier": "1.1.30",
    "@types/jasmine": "2.5.52",
    "@types/node": "8.0.1",
    "@types/webpack-env": "1.13.0",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.3",
    "codelyzer": "2.0.1",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "nodemon": "1.11.0",
    "parallelshell": "3.0.1",
    "protractor": "5.1.2",
    "ts-node": "3.1.0",
    "tslint": "4.5.1",
    "typescript": "2.3.4",
    "webpack-node-externals": "1.6.0"
  }
}
